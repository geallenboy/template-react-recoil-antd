{
  "name": "@garron/template-react17-recoil-antd",
  "version": "1.0.0",
  "description": "基于 vite2 react17 recoil antd 项目模版",
  "scripts": {
    "serve": "npm run dev",
    "start": "vite",
    "build": "cross-env NODE_ENV=production vite build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "deploy": "gh-pages -d dist",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,mock}/**/*.{ts,tsx}\" --fix",
    "lint:lint-staged": "lint-staged",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@antv/g2plot": "^2.4.12",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.8.2",
    "antd": "^4.17.4",
    "axios": "^0.24.0",
    "mockjs": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.1",
    "recoil": "^0.6.1",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@garron/standard": "^1.4.0",
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.21",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-legacy": "^1.6.4",
    "@vitejs/plugin-react": "^1.1.3",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.1.4",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.7",
    "vite-plugin-imp": "^2.0.11",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svgr": "^0.6.0",
    "vite-plugin-windicss": "^1.6.1",
    "windicss": "^3.4.0"
  },
  "author": {
    "name": "garron",
    "url": "https://github.com/geallenboy"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:geallenboy/template-react17-recoil-antd.git"
  },
  "homepage": "https://github.com/geallenboy/template-react17-recoil-antd",
  "keywords": [
    "react",
    "antd",
    "vite2",
    "ts",
    "tsx",
    "recoil",
    "typescript"
  ],
  "license": "MIT",
  "engines": {
    "node": "^12 || >=14"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.tsx": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
